{
  "name": "@audioreach-creator-ui/api-utils",
  "version": "0.0.1",
  "description": "Type definitions and shared utilities for the serverside API in the Electron package.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "run-s clean compile",
    "clean": "shx rm -rf dist",
    "compile": "run-p build:components build:types",
    "build:components": "vite build -c vite.config.ts",
    "build:types": "tsc -b tsconfig.lib.json --force",
    "dev": "run-p watch:components watch:types",
    "watch:components": "vite build -c vite.config.ts -w --emptyOutDir=false",
    "watch:types": "tsc -b tsconfig.lib.json -w --preserveWatchOutput --force",
    "lint": "eslint --quiet"
  },
  "dependencies": {
    "@audioreach-creator-ui/tsconfig": "workspace:*",
    "change-case": "^5.4.4"
  },
  "devDependencies": {
    "@types/node": "^22.16.3",
    "npm-run-all2": "^8.0.4",
    "shx": "^0.4.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vite": "^7.0.4"
  }
}
