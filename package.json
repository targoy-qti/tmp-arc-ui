{
  "name": "audioreach-creator-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "AudioReach Creator to create/view use cases and tune audio modules",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "cross-env FORCE_COLOR=1 turbo run build",
    "build:ci": "cross-env FORCE_COLOR=1 turbo run build",
    "dev": "cross-env FORCE_COLOR=1 DEV=true turbo run dev",
    "dev:ui": "pnpm dev --filter='!@audioreach-creator-ui/electron-app'",
    "dev:electron": "pnpm run -C packages/electron-app dev",
    "typecheck": "cross-env FORCE_COLOR=1 turbo run typecheck",
    "package": "cross-env FORCE_COLOR=1 turbo run package",
    "lint": "cross-env FORCE_COLOR=1 turbo run lint",
    "lint:ci": "cross-env FORCE_COLOR=1 turbo run lint --continue",
    "lint:cache": "eslint --quiet --cache",
    "lint:fix": "eslint . --fix --cache --quiet",
    "test": "cross-env FORCE_COLOR=1 turbo run test --env-mode=loose",
    "test:ci": "cross-env FORCE_COLOR=1 turbo run test:ci --env-mode=loose",
    "release": "cross-env FORCE_COLOR=1 turbo run make",
    "bundle": "run-s build package",
    "update-qui": "pnpm update '@qui/*' -r --latest",
    "api-utils": "pnpm -C packages/api-utils",
    "electron-app": "pnpm -C packages/electron-app",
    "react-app": "pnpm -C packages/react-app",
    "clean": "pnpm run -C packages/electron-app clean && pnpm run -C packages/react-app clean && pnpm run -C packages/api-utils clean"
  },
  "devDependencies": {
    "@qui/eslint-config-fsd": "1.0.1",
    "@qui/eslint-config-mdx": "^3.0.0",
    "@qui/eslint-config-react": "^8.0.0",
    "@qui/eslint-config-typescript": "^10.1.1",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "change-case": "^5.4.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest-junit": "^16.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "stylelint": "^16.21.0",
    "stylelint-prettier": "^5.0.3",
    "syncpack": "^14.0.0-alpha.18",
    "tailwindcss": "^3.4.17",
    "turbo": "^2.5.6",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "packageManager": "pnpm@10.13.1",
  "dependencies": {
    "@xyflow/react": "^12.8.2",
    "elkjs": "^0.11.0"
  }
}
